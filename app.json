{
  "name": "LineBot",
  "description": "Line Bot API",
  "repository": "https://github.com/bowwowxx/linebot",
  "keywords": ["Line", "go", "static"],
  "buildpacks": [
    {
      "url": "https://buildpack-registry.s3.amazonaws.com/buildpacks/heroku"
    },
    {
      "url": "heroku buildpacks:set heroku/ruby"
    }
  ],
  "env": {
    "ChannelAccessToken": {
      "description": "Channel Access Token",
      "required": true
    },
    "ChannelSecret": {
      "description": "Channel Secret",
      "required": true
    }
  }
}
